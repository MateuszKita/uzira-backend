{
  "extends": "tslint:recommended",
  "rules": {
    "member-ordering": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": [
      true,
      "single"
    ],
    "no-console": false,
    "no-unused-variable": true,
    "trailing-comma": false,
    "object-literal-key-quotes": false,
    "max-line-length": [
      true,
      {
        "limit": 180
      }
    ]
  },
  "linterOptions": {
    "exclude": [
      "./node_modules/**"
    ]
  }
}
